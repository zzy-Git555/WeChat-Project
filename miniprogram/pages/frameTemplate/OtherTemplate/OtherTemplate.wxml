<template name="othercontent" data="{{...otherresult}}">

  <view class="page" wx:for="{{otherresult}}" wx:key="index" bindtap="detail" data-result="{{otherresult[index]}}">
    <view class="page__bd">
      <view class="weui-panel weui-panel_access">
        <view class="weui-panel__bd">
          <view class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
            <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
              <image lazy-load mode="scaleToFill" class="weui-media-box__thumb" src="{{item.img[0]}}" />
            </view>
            <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
              <view class="weui-media-box__title">{{item.title}}</view>
              <view class="weui-media-box__desc">{{item.contents}}</view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>

</template>