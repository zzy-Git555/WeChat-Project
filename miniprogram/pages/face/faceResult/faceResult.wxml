<view class="box">
    <image src="{{url}}" mode="widthFix"></image>
</view>


<view class="detail">性别: {{face_list.gender.type == "male" ? "男" : "女"}}</view>
<view class="detail">年龄: {{face_list.age}}</view>
<view class="detail">颜值: {{face_list.beauty}}</view>
<view class="detail">表情: {{face_list.expression.type == "none" ? "不笑" : info.expression.type == "smile" ? "微笑" : "大笑"}}</view>

<view class="detail">情绪: {{face_list.emotion.type == "angry" ? "愤怒" : face_list.emotion.type == "disgust" ? "厌恶" : face_list.emotion.type == "fear" ? "恐惧" : face_list.emotion.type == "happy" ? "高兴" : face_list.emotion.type == "sad" ? "伤心" : face_list.emotion.type == "surprise" ? "惊讶" : face_list.emotion.type == "neutral" ? "无表情" : face_list.emotion.type == "pouty" ? "撅嘴" : "鬼脸"}}</view>

<view class="detail">脸型: {{face_list.face_shape.type == "square" ? "正方形" : face_list.face_shape.type == "triangle" ? "三角形" : face_list.face_shape.type == "oval" ? "椭圆" : face_list.face_shape.type == "heart" ? "心形" : "圆形"}}</view>

<view class="detail">眼镜: {{face_list.glasses.type == "none" ? "无眼镜" : face_list.glasses.type == "common" ? "普通眼镜" : "墨镜"}}</view>

<view class="detail">类别: {{face_list.face_type.type == "human" ? "真实人脸" : "卡通人脸"}}</view>